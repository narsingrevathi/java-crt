import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        int n = 0;
        while (a > 0) {
            int digit = a % 10;
            if (digit % 2 != 0) {
            n += digit;
            }
            a /= 10;
        }
        System.out.println(n);
    
    }
}
