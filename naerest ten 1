import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args){
    Scanner scanner=new Scanner(System.in);
        int a=scanner.nextInt();
        int n=a%10;
        if(n>=5){
            System.out.println(a+(10-n));
        }
        else
        {
            System.out.println(a-n);
        }
    }
}
