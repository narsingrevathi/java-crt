import java.util.*;
class Test
{
Public static void main (string args[  ])
{
	Scanner s=new scanner(System.in);
	int n=s.nextInt( );
	int a[ ]=new int [n];
	for (i=0; i<n; i++)
	{
		a[i]=s.nextInt();
	}
	Arrays.sort(a);
	fo(int i=0; i=n/2; i++ )
	{
		System.out.print(a[n-1-i]+" " +a[i]+" ");
	}
	if(n%2==1)
	{
		System.out.print(a[n/2]);
	}
}
