import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner scanner = new Scanner(System.in);
        int a, sum = 0;
        while (true) {
            a = scanner.nextInt();
            if (a == -1)
                break;
            sum += a;
        }
        System.out.println(sum);
    }
}
