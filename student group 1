import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        if(a>=100&&a<=200)
        {
            int r=a/4;
            System.out.println(r+"\n"+r+"\n"+r+"\n"+(r+a%4));
        }
        else
        {
            System.out.println("INVALID INPUT");
        }
    }
}
