import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int a = 7;
        int b = 5;

        for (int i=1;i<=n;i++) {
            if (i % 2 == 0) {
                System.out.print(b+ " ");
                b++;
            } else {
                System.out.print(a+ " ");
                a++;
            }
        }
    }
}
